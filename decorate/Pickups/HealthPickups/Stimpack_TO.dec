ACTOR Stimpack_TO : Health
{
    Inventory.Amount 10
    Inventory.PickupMessage "$GOTSTIM"
    Inventory.PickupSound "health/stim"
    //Inventory.PickupFlash "Stimpack_ID"
    States
    {
    Spawn:
        STIM A 1 A_SpawnItemEx("HealthPickupChecker", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
        STIM A 30
        STIM A 1
        Loop
    }
}
